@page "/log-migracion"
@layout AdminLayout
@using SharedApp.Dtos
@using SharedApp.Dtos

<PageTitle>Logs de migraciones</PageTitle>

<Grid @ref="grid" TItem="LogMigracionDto" class="table table-hover table-bordered table-striped" DataProvider="LogMigracionDtoDataProvider" Responsive="true">
    <GridColumn TItem="LogMigracionDto" HeaderText="Identificador" PropertyName="Migracion">
        @context.Migracion
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Secuencia" PropertyName="IdLogMigracion">
        @context.IdLogMigracion
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Estado" PropertyName="Estado">
        @context.Estado
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Tipo" PropertyName="OrigenFormato">
        @context.OrigenFormato
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Sistema de origen" PropertyName="OrigenSistema">
        @context.OrigenSistema
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Vista de origen" PropertyName="OrigenVista">
        @context.OrigenVista
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Fecha desde inicio" PropertyName="Inicio">
        @context.Inicio
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Fecha desde fin" PropertyName="Final">
        @context.Final
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Tiempo desde ejecución" PropertyName="Tiempo">
        @context.Tiempo
    </GridColumn>
    <GridColumn TItem="LogMigracionDto" HeaderText="Observación" PropertyName="Observacion">
        @context.Observacion
    </GridColumn>
</Grid>